public class solution {
       public static int findDuplicate(int[] arr) {
              int sum = 0;
              int n = arr.length;
              for(int i = 0; i<n; i++) {
                sum = sum + arr[i];
              ]
              int sum2 = (n - 2) * (n - 1)/2;
              int sub = sum - sum2;
              return sub;
       }
}       
